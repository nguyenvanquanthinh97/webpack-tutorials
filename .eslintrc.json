{
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": 1,
    "no-unused-vars": 1,
    "no-console": 1,
    "indent": [
      1,
      2
    ]
  }
}